/* Header file includes. */
#include "cyhal.h"
#include "cybsp.h"
#include "cy_retarget_io.h"
#include <inttypes.h>

/* FreeRTOS header file. */
#include <FreeRTOS.h>
#include <task.h>
#include <queue.h>

extern QueueHandle_t queue_handle;

#define LED_GPIO CYBSP_USER_LED


void task3(void *arg)
{
	/* Initialize the LED GPIO pin */
	        cyhal_gpio_init(LED_GPIO, CYHAL_GPIO_DIR_OUTPUT, CYHAL_GPIO_DRIVE_STRONG, CYBSP_LED_STATE_OFF);

	for(;;)
	{
		printf("Hello From Task 3\n\r");
		vTaskDelay(pdMS_TO_TICKS(500));
	}
}


